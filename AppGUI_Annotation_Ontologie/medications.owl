<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://www.example.org/medications.owl"
         xmlns="http://www.example.org/medications.owl#">

<owl:Ontology rdf:about="http://www.example.org/medications.owl"/>

<owl:DatatypeProperty rdf:about="#has_quantity">
  <rdfs:domain rdf:resource="#Drug"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#has_side_effect">
